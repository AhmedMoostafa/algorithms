string res;
int n=4, zeros=2;
/* 
0011
0101
0110
1001
1010
1100
*/
void solve(int pos,int cnt)
{
    if (pos == n)   
    {
        if (cnt == zeros)
            cout << res << endl;
    }
    else 
    {
        res += "0";
        solve(pos + 1, cnt);
        res.pop_back();
        res += "1";
        solve(pos + 1, cnt+1);
        res.pop_back();
    }

}
